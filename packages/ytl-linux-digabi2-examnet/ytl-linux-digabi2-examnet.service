[Unit]
Description=Digabi 2 examnet background services

[Service]
Environment="DEBUG=/var/log/ytl-linux-digabi2-examnet-daemon.log"
ExecStart=/usr/local/sbin/ytl-linux-digabi2-examnet --daemon
WorkingDirectory=/tmp
StandardOutput=append:/dev/null
StandardError=append:/dev/null
Restart=always

[Install]
WantedBy=multi-user.target
